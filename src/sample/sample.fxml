<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.TilePane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <Pane mouseTransparent="true" prefHeight="200.0" prefWidth="200.0">
         <children>
            <ListView layoutX="33.0" prefHeight="200.0" prefWidth="200.0" />
         </children>
      </Pane>
   </center>
   <top>
      <Pane maxHeight="30.0" minHeight="30.0" minWidth="250.0" prefWidth="598.0" BorderPane.alignment="CENTER">
         <children>
            <MenuBar maxHeight="30.0" minHeight="30.0" prefHeight="30.0">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </children>
      </Pane>
   </top>
   <left>
      <TilePane maxWidth="50.0" minHeight="150.0" minWidth="50.0" orientation="VERTICAL" prefColumns="1" prefWidth="50.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" text="Button" />
            <Button mnemonicParsing="false" text="Button" />
